export PROJECT=`git rev-parse --show-toplevel`
export PJTMP="${PROJECT}/tmp"
export GIT_USER=`git config user.name`
export GOPROXY="https://proxy.golang.org,direct"
export GOSUMDB="sum.golang.org"

export EMACSD="$HOME/.emacs.d"
export CMACSD="$HOME/.cmacs.d"
export TMPD="$HOME/tmp"

export GO_VERSION=1.23.4
export GOENV_ROOT="$HOME/.goenv"
export GOPATH="$HOME/go"

export PY_VERSION=3.13.1
export PYENV_ROOT="$HOME/.pyenv"

export RB_VERSION=3.2.2
export RBENV_ROOT="$HOME/.rbenv"

export NODE_VERSION=v23.6.0
export NVM_DIR="$HOME/.nvm"
export NPM_ROOT="$HOME/.npm"

export CARGO_HOME="$HOME/.cargo"

export HOMEBREW_BUNDLE_FILE_GLOBAL="$HOME/.Brewfile"
export HOMEBREW_VERIFY_ATTESTATIONS=
